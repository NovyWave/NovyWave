# NovyWave User Configuration
# This file stores your application preferences and workspace state

[app]
version = "1.0.0"

[ui]
theme = "dark"
toast_dismiss_ms = 10000

[workspace]
opened_files = []
dock_mode = "right"
expanded_scopes = []
load_files_expanded_directories = [
    "/home",
    "/home/martinkavik",
    "/home/martinkavik/repos",
    "/home/martinkavik/repos/NovyWave",
    "/home/martinkavik/repos/NovyWave/test_files",
    "/home/martinkavik/repos/NovyWave/test_files/nested_dir",
]
load_files_scroll_position = 845
selected_variables = []
timeline_cursor_position_ns = 0
timeline_zoom_level = 1.0

[workspace.docked_bottom_dimensions]
files_and_scopes_panel_width = 470.0
files_and_scopes_panel_height = 375.0
selected_variables_panel_name_column_width = 338.0
selected_variables_panel_value_column_width = 247.0

[workspace.docked_right_dimensions]
files_and_scopes_panel_width = 528.0
files_and_scopes_panel_height = 278.0
selected_variables_panel_name_column_width = 177.0
selected_variables_panel_value_column_width = 201.0
