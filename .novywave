# NovyWave User Configuration
# This file stores your application preferences and workspace state

[app]
version = "1.0.0"

[ui]
theme = "dark"
toast_dismiss_ms = 5000

[workspace]
opened_files = []
dock_mode = "right"
expanded_scopes = []
load_files_expanded_directories = []
load_files_scroll_position = 0
selected_variables = []
timeline_cursor_position_ns = 0
timeline_zoom_level = 1.0
timeline_visible_range_start_ns = 0
timeline_visible_range_end_ns = 100000000000

[workspace.docked_bottom_dimensions]
files_and_scopes_panel_width = 300.0
files_and_scopes_panel_height = 250.0
selected_variables_panel_name_column_width = 180.0
selected_variables_panel_value_column_width = 100.0

[workspace.docked_right_dimensions]
files_and_scopes_panel_width = 300.0
files_and_scopes_panel_height = 250.0
selected_variables_panel_name_column_width = 180.0
selected_variables_panel_value_column_width = 100.0
